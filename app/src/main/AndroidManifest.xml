<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.reserver.stv2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.STv2"
        >


        <receiver
            android:name=".UI.MyReceiver"
            android:enabled="true"
            android:exported="true" />
        <activity
            android:name=".UI.AlertDetails"
            android:label="@string/notifications"
            android:parentActivityName=".UI.MainActivity"
            android:theme="@style/Theme.STv22" />
        <activity
            android:name=".UI.NoteDetails"
            android:label="@string/notes"
            android:parentActivityName=".UI.MainActivity"
            android:theme="@style/Theme.STv22" />
        <activity
            android:name=".UI.AssessmentDetails"
            android:label="@string/Assessment_Details"
            android:parentActivityName=".UI.MainActivity"
            android:theme="@style/Theme.STv22" />
        <activity
            android:name=".UI.AssessmentList"
            android:label="@string/Assessments"
            android:parentActivityName=".UI.MainActivity"
            android:theme="@style/Theme.STv22" />
        <activity
            android:name=".UI.CourseList"
            android:label="@string/Courses"
            android:parentActivityName=".UI.MainActivity"
            android:theme="@style/Theme.STv22" />
        <activity
            android:name=".UI.TermList"
            android:label="@string/Terms"
            android:parentActivityName=".UI.MainActivity"
            android:theme="@style/Theme.STv22" />
        <activity
            android:name=".UI.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
